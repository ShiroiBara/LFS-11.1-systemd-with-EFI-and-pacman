pkgname=psmisc
pkgver=23.4
pkgrel=1
pkgdesc="Miscellaneous procfs tools"
arch=('x86_64')
url="http://psmisc.sourceforge.net/index.html"
source=($pkgname-$pkgver.tar.xz)
md5sums=('SKIP')
license=('GPL3')

prepare() {

    cd "$srcdir/$pkgname-$pkgver"

}

build() {

    cd "$srcdir/$pkgname-$pkgver"
    ./configure --prefix=/usr
    make

}

check() {

    cd "$srcdir/$pkgname-$pkgver"

}

package() {

    cd "$srcdir/$pkgname-$pkgver"
    make DESTDIR=$pkgdir install

}
